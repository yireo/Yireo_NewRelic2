{
    "name": "yireo/yireo_newrelic2",
    "license": "BSD-2-Clause",
    "type": "magento2-module",
    "version": "0.3.0",
    "homepage": "https://www.yireo.com/software/magento-extensions/newrelic",
    "description": "Add NewRelic JS support and PHP tracking to Magento",
    "keywords":[ "composer-installer", "magento"],
    "authors": [
        {
            "name": "Jisse Reitsma (Yireo)",
            "email": "jisse@yireo.com"
        }
    ],
    "require": {
        "magento/framework": "100.0.*",
        "php": ">=5.6.0"
    },
    "require-dev":{
        "phpunit/phpunit":"*",
        "composer/composer":"*@dev"
    },
    "extra":{
        "map": [
            [
                "source/*",
                "Yireo/NewRelic2"
            ]
        ]
    },
    "autoload": {
        "psr-4": {
            "Yireo\\NewRelic2\\": "source"
        },        
        "files": [
            "source/registration.php"
        ]
    }
}
